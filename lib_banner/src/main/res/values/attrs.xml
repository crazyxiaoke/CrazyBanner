<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CrazyBannerView">
        <attr name="indicator_layout" format="reference" />
        <attr name="indicator_align">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="duration" format="integer" />
        <attr name="autoScroll" format="boolean" />
        <attr name="clipChildren" format="boolean" />
        <attr name="canLoop" format="boolean" />
    </declare-styleable>
</resources>